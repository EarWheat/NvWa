        <spring-cloud-starter-ribbon.version>1.4.2.RELEASE</spring-cloud-starter-ribbon.version>
        <spring-boot-starter-web.version>2.6.1</spring-boot-starter-web.version>
        <spring-cloud-starter-eureka.version>1.4.2.RELEASE</spring-cloud-starter-eureka.version>
        <spring-boot-starter-dubbo.version>3.0.2</spring-boot-starter-dubbo.version>
